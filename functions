SUM function(NANR)
#include <stdio.h>
int sum();
int main()
{
    int result;
    result=sum();
    printf("%d",result);
    
    return 0;
}
int sum(){
    int a,b;
    scanf("%d%d",&a,&b);
    return a+b; 
}

1-
#include <stdio.h>
int sum();
int main()
{
    int result;
    result=sum();
    printf("%d",result);
    
    return 0;
}
int sum(){
    int a;
    scanf("%d",&a);
    return a*a*a; 
}

2-
#include <stdio.h>
int sum();
int main()
{
    int result;
    result=sum();
    printf("%d",result);
    
    return 0;
}
int sum(){
    int a;
    float circumference,area;
    scanf("%d",&a);
    printf("Diameter is %d\n",2*a);
    circumference=2*3.14*a;
    printf("Circumference is %.2f",circumference);
    printf("\n");
    area=3.14*a*a;
    printf("Area is %.2f",area);
    return 0; 
}

3-
#include <stdio.h>
void math2();
int main()
{
    math2();
    
    return 0;
}
void math2(){
    
    int a,b;
    scanf("%d\n%d",&a,&b);
    
    if(a>b){
        printf("Max. is %d and min is %d",a,b);
    }
    else{
        printf("Max. is %d and min. is %d ",b,a);
    }
}

4-
#include <stdio.h>
void math2();
int main()
{
    math2();
    
    return 0;
}
void math2(){
    
    int a;
    scanf("%d",&a);
    
    if(a%2==0){
        printf("Number is even.");
    }
    else{
        printf("Number is odd");
    }
}

5-
#include <stdio.h>
void math2();
int main()
{
    math2();
    
    return 0;
}
void math2(){
    
    int a,b;
    scanf("%d",&a);
    printf("press 153 for Armstrong.");
    printf("press 5 for prime.");
    printf("press 6 for Perfect Number.");
    
    printf("Choose option to check ",&b);
    switch(b){
        case 153:
        int c=0,rem,org;
        org=a;
        while(a!=0){
             rem=org%10;
             c=c+(rem*rem*rem);
             org=org/10;
         }
        if(c==a){
            printf("Number is armstrong.");
        }
        else{
            printf("Not armstrong");
        }
        break;
        
       
    
}
